  </identityField>
  <keyField>
    <field dataField="GUID"/>
    <field dataField="IS_LOCK"/>
  </keyField>
  <allowEmptyField>
  </allowEmptyField>
  <nonUpdateField>     
  </nonUpdateField>
</DataObject>
