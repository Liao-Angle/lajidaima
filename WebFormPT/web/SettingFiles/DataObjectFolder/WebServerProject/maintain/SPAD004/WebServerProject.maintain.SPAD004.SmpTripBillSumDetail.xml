    , s.DATA_STATUS, s.D_INSERTUSER, s.D_INSERTTIME, s.D_MODIFYUSER, s.D_MODIFYTIME  from SmpTripBillSumDetail s
    left join SmpTripBillSummary h on s.SummaryGUID=h.GUID
